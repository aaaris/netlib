     <el-input v-model=\"editform.book_addr\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"出版日期\" prop=\"book_date\">\r\n            <el-form-item prop=\"date1\">\r\n              <el-date-picker\r\n                type=\"date\"\r\n                placeholder=\"选择日期\"\r\n                v-model=\".date1\"\r\n                style=\"width: 100%\"\r\n              ></el-date-picker>\r\n            </el-form-item>\r\n          </el-form-item>\r\n          <el-form-item label=\"书本简介\" prop=\"book_info\">\r\n            <el-input type=\"textarea\" v-model=\"editform.book_info\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" @click=\"onSubmit('editformRef')\"\r\n              >分享</el-button\r\n            >\r\n            <el-button id=\"btu2\" @click=\"resetForm('editformRef')\"\r\n              >重置</el-button\r\n            >\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-card>\r\n      <!-- 用户信息区 -->\r\n      <el-card class=\"user-info-card\">\r\n        <h1>图书照片</h1>\r\n        <el-upload\r\n          class=\"upload-demo\"\r\n          drag\r\n          :action=\"this.$http.defaults.baseURL + `/img/test`\"\r\n          multiple\r\n        >\r\n          <i class=\"el-icon-upload\"></i>\r\n          <div class=\"el-upload__text\">将文件拖到此处，或<em>点击上传</em></div>\r\n          <div class=\"el-upload__tip\" slot=\"tip\">\r\n            只能上传jpg/png文件，且不超过500kb\r\n          </div>\r\n        </el-upload>\r\n      </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      editform: {\r\n        book_name: \"\",\r\n        book_author: \"\",\r\n        book_date: \"\",\r\n        book_addr: \"\",\r\n        book_info: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    onSubmit(formName) {},\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n  },\r\n};\r\n</s