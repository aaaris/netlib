n            <span>书籍出版社：{{ book.book_addr }}</span\r\n            ><br />\r\n            <el-rate\r\n              v-model=\"book_rate\"\r\n              disabled\r\n              show-score\r\n              text-color=\"#ff9900\"\r\n              score-template=\"{value}\"\r\n            >\r\n            </el-rate>\r\n          </div>\r\n        </div>\r\n        <!-- 用户操作按钮 -->\r\n        <div style=\"margin-top: 30px\">\r\n          <el-button icon=\"el-icon-download\" @click=\"\">下载</el-button>\r\n          <el-button icon=\"el-icon-star-off\">收藏</el-button>\r\n        </div>\r\n      </el-card>\r\n      <!-- 内容简介 -->\r\n      <el-card class=\"box-card\">\r\n        <div slot=\"header\" class=\"clearfix\">\r\n          <span class=\"header_name\">内容简介</span>\r\n        </div>\r\n        <div class=\"text item\">\r\n          <p>\r\n            {{ book.book_info }}\r\n          </p>\r\n        </div>\r\n      </el-card>\r\n      <!-- 相关推荐(同类型+同作者) -->\r\n      <el-card class=\"box-card\">\r\n        <div slot=\"header\" class=\"clearfix\">\r\n          <span class=\"header_name\">相关推荐</span>\r\n        </div>\r\n        <div class=\"text item\">\r\n          <p>\r\n            {{ book.info }}\r\n          </p>\r\n        </div>\r\n      </el-card>\r\n      <!-- 评论列表 -->\r\n      <el-card class=\"box-card\">\r\n        <div slot=\"header\" class=\"clearfix\">\r\n          <span class=\"header_name\">评论列表</span>\r\n        </div>\r\n        <div class=\"text item\">\r\n          <p>\r\n            {{ book.info }}\r\n          </p>\r\n        </div>\r\n      </el-card>\r\n      <!-- 发表评论 -->\r\n      <el-card class=\"box-card\">\r\n        <div slot=\"header\" class=\"clearfix\">\r\n          <span class=\"header_name\">发表评论</span>\r\n        </div>\r\n        <div class=\"text item\">\r\n          <p>\r\n            {{ book.info }}\r\n          </p>\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n    <Footer></Footer>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Header from \"../Header.vue\";\r\nimport Footer from \"../Footer.vue\";\r\nexport default {\r\n  components: { Header, Footer },\r\n  async created() {\r\n    const { data: res } = await this.$http.get(\r\n      \"/book/\" + this.$route.params.book_id\r\n    );\r\n    this.book = res.data;\r\n    console.log(this.book);\r\n  },\r\n  data() {\r\n    return {\r\n      book: {\r\n        book_addr: \"\",\r\n        book_author: \"\",\r\n        book_date: \"\",\r\n        book_dl_count: \"\",\r\n        book_id: \"\",\r\n        book_img_url: \"\",\r\n        book_info: \"\",\r\n        book_name: \"\",\r\n        book_score: \"\",\r\n        class_name: \"\",\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    book_rate: {\r\n      get() {\r\n        return this.book.book_score * 0.5;\r\n      },\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n// 内容容器\r\n.main-container {\r\n  line-height: 30px;\r\n  width: 75%;\r\n  margin: 50px auto;\r\n  // background-color: white;\r\n  // box-shadow: 0 4px 14px 0 rgb(0 0 0 / 5%);\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n  display: table;\r\n  content: \"\";\r\n}\r\n.clearfix:after {\r\n  clear: both;\r\n}\r\n.clearfix {\r\n  color: #777;\r\n  span {\r\n    font-size: 20px;\r\n  }\r\n}\r\n\r\n.box-card {\r\n  // width: 480px;\r\n  margin-top: 30px;\r\n}\r\n\r\n// 移动端适配\r\n@media (max-width: 600px) {\r\n  .main-container {\r\n    margin: 30px auto;\r\n    width: 90%;\r\n    padding: 10px;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}